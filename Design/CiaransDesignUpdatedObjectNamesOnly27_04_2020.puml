@startuml





/' Objects '/
namespace Legend {
    class Parent
    class Child
    class Component
    class LargerObject

    Parent <|-- Child : Inheritance relationship
    LargerObject "1" *-- "many" Component : Aggregation relationship
    note right of LargerObject
        The LargerObject has
        a "has-a" 1 to many
        relationship with
        Component
    end note
    note left of Parent
        The child has
        a "is-a"  relationship
        with Parent.
    end note

}

    together {
        class PhysicalForce
        class PhysicalEntity
        class PhysicalProcess
    }

    together {
        class Editor
        class Reader
        class Writer
    }

    together {
        class MetaID
        class CellMLAssistant
        class SBMLAssistant
    }


